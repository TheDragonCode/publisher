{
    "name": "andrey-helldar/publisher",
    "description": "Simple publication and revoke of releases",
    "type": "composer-plugin",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "composer-plugin-api": "^1.1",
        "composer/composer": "*"
    },
    "autoload": {
        "psr-4": {
            "Helldar\\Publisher\\": "src/"
        }
    },
    "extra": {
        "class": "Helldar\\Publisher\\Publisher"
    },
    "config": {
        "sort-packages": true
    }
}
